//WRITE A C PROGRAM TO FIND THAT THE ACCEPTEDNUMBER IS NEGATIVE OR POSITIVE OR ZERO

#include <stdio.h>

int main()
{
    
    int a;
    printf("enter a:");
    scanf("%d",&a);
    if(a>=0)
       printf("%d is a positive",a);
    else 
       printf("%d is a negative",a);
    
    
   
  
    return 0;
}





